<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping >
	<class name="com.moblie.cn.domain.Admin" table="admin">
		<id name="admin_id" type="integer">
		    <column name="admin_id" />
			<generator class="native"/>
		</id>
		<property name="admin_name" type="string" column="admin_name"/>
		<property name="admin_pwd" type="string" column="admin_pwd"/>
		<property name="admin_img" type="string" column="admin_img"/>
	</class>
</hibernate-mapping>